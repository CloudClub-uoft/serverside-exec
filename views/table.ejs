<html>
	<link
		rel="stylesheet"
		href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
		integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
		crossorigin="anonymous"
	/>

	<div class="container-fluid">
		<div class="row">
			<div class="col-md-12">
				<h3>
					<a href="/"> CloudFunctions </a>
				</h3>
			</div>
		</div>
	</div>

	<div class="container-fluid">
		<div class="row">
			<div class="col-md-12">
				<div class="jumbotron">					
					<p>Functions</p>
					
					<table class="table">
						<thead>
						  <tr>
							<th scope="col">Name</th>
							<th scope="col">Link</th>
						  </tr>
						</thead>
						<tbody>
							<% for(var key in functions) { %>
								<tr>
									<th scope="row"><%- key -%></th>
									<td><a href="/function/<%-key-%>">/function/<%-key-%></a></td>							
								</tr>		
							 <% } %>						  				
						</tbody>
					  </table>
					
				</div>
			</div>
		</div>
	</div>

</html>
